{
  "$id": "Asset Schema",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "A representation of an asset",
  "type": "object",
  "additionalProperties": false,
  "required": ["assetTag", "name"],
  "properties": {
    "id": {
      "type": "number"
    },
    "assetTag": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "serialNumber": {
      "type": "string"
    },
    "modelNumber": {
      "type": "string"
    },
    "nextAuditDate": {
      "type": "string",
       "format": "date-string"
    },
    "createdAt": {
      "type": "string",
      "format": "date-string"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-string"
    }
  }
}
